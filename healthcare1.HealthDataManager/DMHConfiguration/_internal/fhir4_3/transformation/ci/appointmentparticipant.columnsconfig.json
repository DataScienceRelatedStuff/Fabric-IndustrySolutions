      "name": "Id",
      "style": "expr",
      "expression": "concat(id_orig,'-',index)"
    },
    {
      "name": "AppointmentId",
      "style": "expr",
      "expression": "id_orig"
    },
    {
      "name": "Type",
      "style": "expr",
      "expression": "participant_expl.type.coding[0].code[0]"
    },
    {
      "name": "ParticipantId",
      "style": "expr",
      "expression": "participant_expl.actor.id_orig"
    },
    {
      "name": "ParticipantType",
      "style": "expr",
      "expression": "participant_expl.actor.type"
    },
    {
      "name": "LastUpdated",
      "style": "col",
      "expression": "meta.lastUpdated"
    }
  ]
}